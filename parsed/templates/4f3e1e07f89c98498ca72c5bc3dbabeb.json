{
  "components/f/hero/carousel": "<section class=\"heroCarousel{{#if theme_settings.show-login-on-homepage}}{{#unless customer}} heroCarousel--with-login{{/unless}}{{/if}}\"\n    data-slick='{\n        \"arrows\": {{arrows}},\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"autoplaySpeed\": {{carousel.swap_frequency}},\n        \"lazyLoad\": \"anticipated\",\n        \"arrows\": false,\n        \"dots\": false,\n        \"responsive\": [\n            {\n                \"breakpoint\": 800,\n                \"settings\": {\n                    \"arrows\": true,\n                    \"dots\": true\n                }\n            }\n        ]\n    }'>\n    {{#each carousel.slides}}\n    <a href=\"{{url}}\" class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\n        <div class=\"heroCarousel-image-wrapper\" {{#and image_height image_width}}style=\"height: {{multiply (divide image_height image_width) 100}}vw\"{{/and}}>\n            <img class=\"heroCarousel-image\" {{#if @first}}src{{else}}data-lazy{{/if}}=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"\n            {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n        </div>\n        {{#if heading}}\n            {{> components/f/hero/carousel-content show_background=true}}\n        {{else}}\n            {{#if text}}\n                {{> components/f/hero/carousel-content show_background=true}}\n            {{else}}\n                {{#if button_text}}\n                    {{> components/f/hero/carousel-content show_background=false}}\n                {{/if}}\n            {{/if}}\n        {{/if}}\n    </a>\n    {{/each}}\n</section>\n",
  "components/f/hero/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\n    {{#if button_text}}\n        <span class=\"heroCarousel-action button button--primary button--large\">{{button_text}}</span>\n    {{/if}}\n</div>\n"
}
