{
  "components/f/blog/home-articles-list": "<h2 class=\"page-heading page-heading--center\">{{lang 'blog.recent_posts'}}</h2>\n<ul class=\"blogPosts postColumns--{{columns}}\" data-slick='{\n    \"dots\": true,\n    \"infinite\": false,\n    \"mobileFirst\": true,\n    \"slidesToShow\": 1,\n    \"slidesToScroll\": 1,\n    \"responsive\": [\n        {\n            \"breakpoint\": 800,\n            \"settings\":  \"unslick\"\n        },\n        {\n            \"breakpoint\": 550,\n            \"settings\": {\n                \"slidesToScroll\": 2,\n                \"slidesToShow\": 2\n            }\n        }\n    ]\n}'>\n    {{#each posts}}\n        {{> components/f/blog/blog-post-card theme_settings=../theme_settings}}\n    {{/each}}\n</ul>\n<div class=\"blogPosts-more\">\n    <a href=\"{{blog.url}}\">{{lang 'blog.view_all'}}</a>\n</div>\n",
  "components/f/blog/blog-post-card": "<li class=\"blogPost\">\n    <article class=\"card card--blogPost\">\n        {{#if thumbnail.data}}\n            <figure class=\"blogPost-image\">\n                <a href=\"{{url}}\">\n                    <div class=\"blogPost-img-container\">\n                        <img class=\"blogPost-thumbnail lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{#replace \"{:size}\" thumbnail.data}}{{theme_settings.homepage_blog_size}}{{/replace}}\" alt=\"{{thumbnail.alt}}\">\n                    </div>\n                </a>\n            </figure>\n        {{/if}}\n        <div class=\"card-body card-body--blogPost\">\n            <p class=\"card-text card-text--blogPost\">{{date_published}}</p>\n            <h4 class=\"card-title card-title--blogPost\"><a href=\"{{url}}\">{{title}}</a></h4>\n            <p class=\"card-text card-text--summary\">{{{ellipsis summary}}}</p>\n            {{#if author}}\n                <p class=\"card-text card-text--blogPost\">{{lang 'blog.posted_by' name=author}}</p>\n            {{/if}}\n        </div>\n    </article>\n</li>\n"
}
