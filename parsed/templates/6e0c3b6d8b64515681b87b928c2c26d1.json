{
  "components/account/orders-list": "<ul class=\"account-list\">\n    {{#each orders}}\n        <li class=\"account-listItem microsoft-row-generator-orders--{{@index}}\">\n            <div class=\"account-product account-orders\">\n                <div class=\"account-product-detail order-placed\">\n                    <h6 class=\"account-product-detail-heading\">{{lang 'account.orders.order_placed' }}</h6>\n                    <span>{{this.date}}</span>\n                </div>\n\n                <div class=\"account-product-detail order-total\">\n                    <h6 class=\"account-product-detail-heading\">{{lang 'b2b.account.orders.total' }}</h6>\n                    <span>{{this.total.formatted}}</span>\n                </div>\n\n                <div class=\"account-product-detail order-status\">\n                    <h6 class=\"account-product-detail-heading\n                    {{#if status '==' }}\n                    {{/if}}\">{{lang 'b2b.account.orders.status' }}</h6>\n                    <div class=\"account-orderStatus\">\n                        <h6 class=\"account-orderStatus-label\">{{this.status}}</h6>\n                    </div>\n                </div>\n\n                <div class=\"account-product-detail order-number\">\n                    <div>{{lang 'b2b.account.orders.id' }}{{this.id}}</div>\n                    <div class=\"order-actions\">\n                        {{#if this.details_url}}\n                            <a href=\"{{this.details_url}}\">\n                                <span>{{lang 'b2b.account.orders.details' }}</span>\n                            </a>\n                        {{/if}}\n                        <a data-print-invoice=\"/account.php?action=print_invoice&order_id={{this.id}}\">\n                            <span>{{lang 'b2b.account.orders.invoice'}}</span>\n                        </a>\n                    </div>\n                </div>\n\n                {{#each items}}\n                    <div class=\"account-product-item microsoft-row-generator-orders--{{@index}}\">\n                        <div class=\"account-product-figure\">\n                            {{#if items.0.type '===' 'giftcertificate'}}\n                                <img class=\"account-product-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{cdn ../../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\n                            {{else}}\n                                <img class=\"account-product-image lazyload\"\n                                     data-sizes=\"auto\"\n                                     src=\"{{cdn 'img/loading.svg'}}\"\n                                     data-src=\"{{getImage image 'productthumb_size' (cdn ../../../theme_settings.default_image_product)}}\"\n                                     alt=\"{{image.alt}}\"\n                                     title=\"{{image.alt}}\">\n                            {{/if}}\n                        </div>\n\n                        <div class=\"account-product-order-item\">\n                            <span>{{quantity}} &#215; {{ellipsis name 52}}\n                                {{#if refunded '==' 0}}\n                                    {{#if download_url}}\n                                        <a class=\"order-item-download\" href=\"{{download_url}}\">{{lang 'b2b.account.orders.download'}}</a>\n                                    {{/if}}\n                                {{/if}}\n                            </span>\n                        </div>\n                    </div>\n                {{/each}}\n            </div>\n        </li>\n    {{/each}}\n</ul>\n"
}
